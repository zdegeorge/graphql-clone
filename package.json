{
  "name": "ziggy-graphql",
  "version": "1.0.0",
  "description": "Ziggy is a development tool that is meant to store and retrieve bits of code and other references that may need to be quickly accessed",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zdegeorge/ziggy.git"
  },
  "scripts": {
    "build": "tsc",
    "serve": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "script:generate-schema": "ts-node ./scripts/generate-schema",
    "schema": "npm run script:generate-schema && npm run generate",
    "generate_orig": "yarn graphql-codegen --config codegen.yml",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
