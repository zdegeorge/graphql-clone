    "@types/knex": "^0.16.1",
    "@types/mysql": "^2.15.16",
    "apollo-server": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "apollo-server-plugin-base": "^3.4.0",
    "apollo-server-plugin-response-cache": "^3.4.0",
    "dotenv": "^10.0.0",
    "graphql": "^16.2.0",
    "graphql-fields": "^2.0.3",
    "graphql-query-complexity": "^0.7.2",
    "graphql-scalars": "^1.14.1",
    "graphql-tools": "^4.0.8",
